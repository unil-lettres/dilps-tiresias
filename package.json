{
    "name": "dilps-tiresias",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "dev": "yarn codegen && yarn set-env && yarn clean-env && ng serve",
        "dev-docker": "yarn codegen && yarn set-env && yarn clean-env && ng build --configuration=development --watch --output-hashing=all",
        "prod": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production",
        "staging": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging",
        "test": "yarn codegen && ng test",
        "lint": "ng lint",
        "e2e": "playwright test",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js --passthroughCustomScalars --customScalarsPrefix=App --outputFlat --target typescript client/app/shared/generated-types.ts",
        "set-env": "ts-node scripts/setenv.ts",
        "clean-env": "prettier --write client/environments/*",
        "report-build": "bugsnag-build-reporter -n 'Dilps/Tiresias Deployer'"
    },
    "dependencies": {
        "@angular/animations": "^13.2.5",
        "@angular/cdk": "^13.2.5",
        "@angular/common": "^13.2.5",
        "@angular/compiler": "^13.2.5",
        "@angular/core": "^13.2.5",
        "@angular/flex-layout": "^13.0.0-beta.38",
        "@angular/forms": "^13.2.5",
        "@angular/google-maps": "^13.2.5",
        "@angular/localize": "^13.2.5",
        "@angular/material": "^13.2.5",
        "@angular/platform-browser": "^13.2.5",
        "@angular/platform-browser-dynamic": "^13.2.5",
        "@angular/router": "^13.2.5",
        "@apollo/client": "^3.4.16",
        "@bugsnag/core": "^7.10.0",
        "@bugsnag/js": "^7.10.1",
        "@bugsnag/plugin-angular": "^7.10.1",
        "@ecodev/angular-natural-gallery": "^12.0.0",
        "@ecodev/natural": "^42.4.1",
        "@graphql-tools/mock": "^8.4.0",
        "@types/swiper": "^6.0.0",
        "apollo": "^2.34.0",
        "apollo-angular": "^3.0.0",
        "apollo-upload-client": "^17.0.0",
        "bugsnag-build-reporter": "^2.0.0",
        "dotenv": "^16.0.0",
        "graphql": "^15.6.1",
        "highcharts": "^9.1.0",
        "highcharts-angular": "^3.0.0",
        "lodash-es": "^4.17.21",
        "material-design-icons": "^3.0.1",
        "ngx-perfect-scrollbar": "^10.1.1",
        "ngx-progressbar": "^8.0.0",
        "ngx-quill": "^16.1.2",
        "photoswipe": "^4.1.3",
        "quill": "^1.3.7",
        "rxjs": "^7.4.0",
        "shelljs": "^0.8.4",
        "snazzy-info-window": "^1.1.1",
        "striptags": "^3.2.0",
        "swiper": "^8.0.6",
        "tslib": "^2.2.0",
        "yargs": "^17.2.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.2.5",
        "@angular-eslint/builder": "13.1.0",
        "@angular-eslint/eslint-plugin": "13.1.0",
        "@angular-eslint/eslint-plugin-template": "13.1.0",
        "@angular-eslint/schematics": "13.1.0",
        "@angular-eslint/template-parser": "13.1.0",
        "@angular/cli": "^13.2.5",
        "@angular/compiler-cli": "^13.2.5",
        "@playwright/test": "^1.15.2",
        "@types/apollo-upload-client": "^17.0.0",
        "@types/googlemaps": "^3.43.3",
        "@types/jasmine": "~3.10.3",
        "@types/jasminewd2": "^2.0.9",
        "@types/lodash-es": "^4.17.3",
        "@types/node": "^14.14.31",
        "@types/quill": "1.3.10",
        "@typescript-eslint/eslint-plugin": "5.13.0",
        "@typescript-eslint/parser": "5.13.0",
        "eslint": "^8.2.0",
        "jasmine-core": "~4.0.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "^6.3.14",
        "karma-chrome-launcher": "~3.1.0",
        "karma-cli": "^2.0.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.6.0",
        "prettier": "2.5.1",
        "ts-node": "^10.0.0",
        "typescript": "~4.5.5"
    }
}

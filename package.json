        "jasmine-spec-reporter": "~7.0.0",
        "karma": "^6.3.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-cli": "^2.0.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.6.0",
        "prettier": "2.4.1",
        "ts-node": "^10.0.0",
        "tslint": "~6.1.0",
        "typescript": "~4.3.5"
    }
}

{
    "name": "dilps-tiresias",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "dev": "yarn codegen && yarn set-env && yarn clean-env && ng serve",
        "dev-docker": "yarn codegen && yarn set-env && yarn clean-env && ng build --configuration=development --watch --output-hashing=all",
        "prod": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production",
        "prod-map": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production,map --output-hashing=none",
        "staging": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging",
        "staging-map": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging,map --output-hashing=none",
        "test": "yarn codegen && ng test",
        "lint": "ng lint --max-warnings 0",
        "e2e": "playwright test",
        "codegen": "./bin/dump-schema && graphql-codegen --config codegen.ts",
        "set-env": "ts-node scripts/setenv.ts",
        "clean-env": "prettier --write client/environments/*",
        "report-build": "bugsnag-build-reporter -n 'Dilps/Tiresias Deployer'"
    },
    "dependencies": {
        "@angular/animations": "^16.1.8",
        "@angular/cdk": "^16.1.7",
        "@angular/common": "^16.1.8",
        "@angular/compiler": "^16.1.8",
        "@angular/core": "^16.1.8",
        "@angular/forms": "^16.1.8",
        "@angular/google-maps": "16.1.7",
        "@angular/localize": "^16.1.8",
        "@angular/material": "^16.1.7",
        "@angular/platform-browser": "^16.1.8",
        "@angular/platform-browser-dynamic": "^16.1.8",
        "@angular/router": "^16.1.8",
        "@apollo/client": "~3.7.17",
        "@bugsnag/core": "^7.19.0",
        "@bugsnag/js": "^7.19.0",
        "@bugsnag/plugin-angular": "^7.19.0",
        "@ecodev/angular-natural-gallery": "^18.0.0",
        "@ecodev/natural": "^55.1.1",
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/typescript-apollo-angular": "^3.5.6",
        "@graphql-codegen/typescript-operations": "^4.0.1",
        "@graphql-tools/mock": "^9.0.0",
        "@ngbracket/ngx-layout": "^16.0.0",
        "@types/googlemaps": "^3.43.3",
        "apollo-angular": "^5.0.0",
        "bugsnag-build-reporter": "^2.0.0",
        "dotenv": "^16.3.1",
        "graphql": "^16.8.1",
        "highcharts": "^11.1.0",
        "highcharts-angular": "^3.0.0",
        "lodash-es": "^4.17.21",
        "ngx-progressbar": "^11.0.0",
        "ngx-quill": "^22.0.0",
        "ngx-scrollbar": "^13.0.3",
        "quill": "^1.3.7",
        "rxjs": "^7.8.0",
        "shelljs": "^0.8.4",
        "snazzy-info-window": "^1.1.1",
        "striptags": "^3.2.0",
        "swiper": "^10.1.0",
        "tslib": "^2.6.1",
        "yargs": "^17.6.2",
        "zone.js": "~0.13.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.1.8",
        "@angular-eslint/builder": "16.1.0",
        "@angular-eslint/eslint-plugin": "16.1.0",
        "@angular-eslint/eslint-plugin-template": "16.1.0",
        "@angular-eslint/schematics": "16.1.0",
        "@angular-eslint/template-parser": "16.1.0",
        "@angular/cli": "^16.1.8",
        "@angular/compiler-cli": "^16.1.8",
        "@playwright/test": "^1.36.2",
        "@types/jasmine": "~4.3.5",
        "@types/jasminewd2": "^2.0.9",
        "@types/lodash-es": "^4.17.8",
        "@types/node": "^18.0",
        "@types/quill": "1.3.10",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "eslint": "^8.46.0",
        "jasmine-core": "~5.1.0",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "^6.4.1",
        "karma-chrome-launcher": "~3.2.0",
        "karma-cli": "^2.0.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "prettier": "3.0.1",
        "ts-node": "^10.0.0",
        "typescript": "~5.1.6"
    }
}

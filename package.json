{
    "name": "dilps-tiresias",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "dev": "yarn codegen && yarn set-env && yarn clean-env && ng serve",
        "dev-docker": "yarn codegen && yarn set-env && yarn clean-env && ng build --configuration=development --watch --output-hashing=all",
        "prod": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production",
        "staging": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging",
        "test": "yarn codegen && ng test",
        "lint": "ng lint --max-warnings 0",
        "e2e": "playwright test",
        "codegen": "./bin/dump-schema && apollo client:codegen --suppressDeprecationWarning -c apollo.config.js --passthroughCustomScalars --customScalarsPrefix=App --outputFlat --target typescript client/app/shared/generated-types.ts",
        "set-env": "ts-node scripts/setenv.ts",
        "clean-env": "prettier --write client/environments/*",
        "report-build": "bugsnag-build-reporter -n 'Dilps/Tiresias Deployer'"
    },
    "resolutions": {
        "graphql": "^15"
    },
    "dependencies": {
        "@angular/animations": "^15.1.2",
        "@angular/cdk": "^15.1.2",
        "@angular/common": "^15.1.2",
        "@angular/compiler": "^15.1.2",
        "@angular/core": "^15.1.2",
        "@angular/flex-layout": "^15.0.0-beta.42",
        "@angular/forms": "^15.1.2",
        "@angular/google-maps": "15.1.2",
        "@angular/localize": "^15.1.2",
        "@angular/material": "^15.1.2",
        "@angular/platform-browser": "^15.1.2",
        "@angular/platform-browser-dynamic": "^15.1.2",
        "@angular/router": "^15.1.2",
        "@apollo/client": "^3.7.5",
        "@bugsnag/core": "^7.19.0",
        "@bugsnag/js": "^7.19.0",
        "@bugsnag/plugin-angular": "^7.19.0",
        "@ecodev/angular-natural-gallery": "^16.0.0",
        "@ecodev/natural": "^50.0.2",
        "@graphql-tools/mock": "^8.7.16",
        "@types/swiper": "^6.0.0",
        "apollo": "^2.34.0",
        "apollo-angular": "^4.2.0",
        "apollo-upload-client": "^17.0.0",
        "bugsnag-build-reporter": "^2.0.0",
        "dotenv": "^16.0.3",
        "graphql": "^15",
        "highcharts": "^10.3.3",
        "highcharts-angular": "^3.0.0",
        "lodash-es": "^4.17.21",
        "material-design-icons": "^3.0.1",
        "ngx-perfect-scrollbar": "^10.1.1",
        "ngx-progressbar": "^9.0.0",
        "ngx-quill": "^20.0.1",
        "quill": "^1.3.7",
        "rxjs": "^7.8.0",
        "shelljs": "^0.8.4",
        "snazzy-info-window": "^1.1.1",
        "striptags": "^3.2.0",
        "swiper": "^8.4.7",
        "tslib": "^2.5.0",
        "yargs": "^17.6.2",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.1.3",
        "@angular-eslint/builder": "15.2.0",
        "@angular-eslint/eslint-plugin": "15.2.0",
        "@angular-eslint/eslint-plugin-template": "15.2.0",
        "@angular-eslint/schematics": "15.2.0",
        "@angular-eslint/template-parser": "15.2.0",
        "@angular/cli": "^15.1.3",
        "@angular/compiler-cli": "^15.1.2",
        "@playwright/test": "^1.30.0",
        "@types/apollo-upload-client": "^17.0.2",
        "@types/googlemaps": "^3.43.3",
        "@types/jasmine": "~4.3.1",
        "@types/jasminewd2": "^2.0.9",
        "@types/lodash-es": "^4.17.3",
        "@types/node": "^18.11.18",
        "@types/quill": "1.3.10",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "eslint": "^8.33.0",
        "jasmine-core": "~4.5.0",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "^6.4.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-cli": "^2.0.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "prettier": "2.8.3",
        "ts-node": "^10.0.0",
        "typescript": "~4.9.5"
    }
}

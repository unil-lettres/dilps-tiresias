{
    "name": "dilps-tiresias",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "dev": "yarn codegen && yarn set-env && yarn clean-env && ng serve",
        "dev-docker": "yarn codegen && yarn set-env && yarn clean-env && ng build --configuration=development --watch --output-hashing=all",
        "prod": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production",
        "prod-map": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production,map --output-hashing=none",
        "staging": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging",
        "staging-map": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging,map --output-hashing=none",
        "test": "yarn codegen && ng test",
        "lint": "ng lint --max-warnings 0",
        "e2e": "playwright test",
        "codegen": "./bin/dump-schema && graphql-codegen --config codegen.ts",
        "set-env": "ts-node scripts/setenv.ts",
        "clean-env": "prettier --write client/environments/*",
        "report-build": "bugsnag-build-reporter -n 'Dilps/Tiresias Deployer'"
    },
    "dependencies": {
        "@angular/animations": "^19.2.13",
        "@angular/cdk": "^19.2.17",
        "@angular/common": "^19.2.13",
        "@angular/compiler": "^19.2.13",
        "@angular/core": "^19.2.13",
        "@angular/forms": "^19.2.13",
        "@angular/google-maps": "19.2.9",
        "@angular/localize": "^19.2.13",
        "@angular/material": "^19.2.17",
        "@angular/platform-browser": "^19.2.13",
        "@angular/platform-browser-dynamic": "^19.2.13",
        "@angular/router": "^19.2.13",
        "@apollo/client": "^3.13.7",
        "@bugsnag/core": "^8.2.0",
        "@bugsnag/js": "^8.2.0",
        "@bugsnag/plugin-angular": "^8.3.0",
        "@ecodev/angular-natural-gallery": "^22.0.0",
        "@ecodev/natural": "^63.3.3",
        "@ecodev/natural-layout": "^2.0.2",
        "@graphql-codegen/cli": "^5.0.5",
        "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
        "@graphql-codegen/typescript-operations": "^4.6.0",
        "@graphql-tools/mock": "^9.0.22",
        "@types/googlemaps": "^3.43.3",
        "apollo-angular": "^10.0.3",
        "bugsnag-build-reporter": "^2.0.0",
        "dotenv": "^16.5.0",
        "graphql": "^16.10.0",
        "highcharts": "^12.2.0",
        "highcharts-angular": "^5.0.0",
        "lodash-es": "^4.17.21",
        "ngx-progressbar": "^14.0.0",
        "ngx-quill": "^27.0.2",
        "ngx-scrollbar": "^18.0.0",
        "quill": "^2.0.3",
        "rxjs": "^7.8.2",
        "snazzy-info-window": "^1.1.1",
        "striptags": "^4.0.0-alpha.4",
        "swiper": "^11.2.6",
        "tslib": "^2.8.1",
        "type-fest": "^4.39.1",
        "yargs": "^17.6.2",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.2.13",
        "@angular/cli": "^19.2.13",
        "@angular/compiler-cli": "^19.2.13",
        "@playwright/test": "^1.51.1",
        "@types/jasmine": "~5.1.7",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.14.1",
        "angular-eslint": "^19.3.0",
        "eslint": "^9.24.0",
        "jasmine-core": "~5.6.0",
        "karma": "^6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-cli": "^2.0.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "postcss": "^8.5.3",
        "prettier": "3.5.3",
        "prettier-plugin-css-order": "^2.1.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.29.1"
    }
}

{
    "name": "dilps-tiresias",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "dev": "yarn codegen && yarn set-env && yarn clean-env && ng serve",
        "dev-docker": "yarn codegen && yarn set-env && yarn clean-env && ng build --configuration=development --watch --output-hashing=all",
        "prod": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production",
        "staging": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging",
        "test": "yarn codegen && ng test",
        "lint": "ng lint",
        "e2e": "playwright test",
        "codegen": "./bin/dump-schema && apollo client:codegen -c apollo.config.js --passthroughCustomScalars --customScalarsPrefix=App --outputFlat --target typescript client/app/shared/generated-types.ts",
        "set-env": "ts-node scripts/setenv.ts",
        "clean-env": "prettier --write client/environments/*",
        "report-build": "bugsnag-build-reporter -n 'Dilps/Tiresias Deployer'"
    },
    "dependencies": {
        "@agm/core": "^3.0.0-beta.0",
        "@agm/snazzy-info-window": "^3.0.0-beta.0",
        "@angular/animations": "^12.2.9",
        "@angular/cdk": "^12.2.9",
        "@angular/common": "^12.2.9",
        "@angular/compiler": "^12.2.9",
        "@angular/core": "^12.2.9",
        "@angular/flex-layout": "^12.0.0-beta.34",
        "@angular/forms": "^12.2.9",
        "@angular/localize": "^12.2.9",
        "@angular/material": "^12.2.9",
        "@angular/platform-browser": "^12.2.9",
        "@angular/platform-browser-dynamic": "^12.2.9",
        "@angular/router": "^12.2.9",
        "@apollo/client": "^3.4.16",
        "@bugsnag/core": "^7.10.0",
        "@bugsnag/js": "^7.10.1",
        "@bugsnag/plugin-angular": "^7.10.1",
        "@ecodev/angular-natural-gallery": "^11.0.0",
        "@ecodev/natural": "^38.1.0",
        "@graphql-tools/mock": "^8.4.0",
        "@types/swiper": "^5.4.2",
        "apollo": "^2.33.4",
        "apollo-angular": "^2.6.0",
        "apollo-upload-client": "^16.0.0",
        "bugsnag-build-reporter": "^2.0.0",
        "dotenv": "^10.0.0",
        "graphql": "^15.6.1",
        "highcharts": "^9.1.0",
        "highcharts-angular": "^2.10.0",
        "lodash-es": "^4.17.21",
        "material-design-icons": "^3.0.1",
        "ngx-perfect-scrollbar": "^10.1.1",
        "ngx-progressbar": "^6.1.1",
        "ngx-quill": "^14.0.0",
        "ngx-swiper-wrapper": "^10.0.0",
        "photoswipe": "^4.1.3",
        "quill": "^1.3.7",
        "rxjs": "^7.4.0",
        "shelljs": "^0.8.4",
        "snazzy-info-window": "^1.1.1",
        "striptags": "^3.2.0",
        "swiper": "^6.6.2",
        "tslib": "^2.2.0",
        "yargs": "^17.2.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.2.9",
        "@angular/cli": "^12.2.9",
        "@angular/compiler-cli": "^12.2.9",
        "@playwright/test": "^1.15.2",
        "@types/apollo-upload-client": "^14.1.0",
        "@types/googlemaps": "3.39.13",
        "@types/jasmine": "~3.9.1",
        "@types/jasminewd2": "^2.0.9",

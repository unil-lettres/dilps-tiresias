{
    "name": "dilps-tiresias",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "dev": "yarn codegen && yarn set-env && yarn clean-env && ng serve",
        "dev-docker": "yarn codegen && yarn set-env && yarn clean-env && ng build --configuration=development --watch --output-hashing=all",
        "prod": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production",
        "prod-map": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production,map --output-hashing=none",
        "staging": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging",
        "staging-map": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging,map --output-hashing=none",
        "test": "yarn codegen && ng test",
        "lint": "ng lint --max-warnings 0",
        "e2e": "playwright test",
        "codegen": "./bin/dump-schema && graphql-codegen --config codegen.ts",
        "set-env": "ts-node scripts/setenv.ts",
        "clean-env": "prettier --write client/environments/*",
        "report-build": "bugsnag-build-reporter -n 'Dilps/Tiresias Deployer'"
    },
    "dependencies": {
        "@angular/animations": "^17.3.4",
        "@angular/cdk": "^17.3.4",
        "@angular/common": "^17.3.4",
        "@angular/compiler": "^17.3.4",
        "@angular/core": "^17.3.4",
        "@angular/forms": "^17.3.4",
        "@angular/google-maps": "17.3.4",
        "@angular/localize": "^17.3.4",
        "@angular/material": "^17.3.4",
        "@angular/platform-browser": "^17.3.4",
        "@angular/platform-browser-dynamic": "^17.3.4",
        "@angular/router": "^17.3.4",
        "@apollo/client": "~3.9.5",
        "@bugsnag/core": "^7.19.0",
        "@bugsnag/js": "^7.22.4",
        "@bugsnag/plugin-angular": "^7.22.4",
        "@ecodev/angular-natural-gallery": "^20.0.0",
        "@ecodev/natural": "^57.0.6",
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
        "@graphql-codegen/typescript-operations": "^4.2.0",
        "@graphql-tools/mock": "^9.0.2",
        "@ngbracket/ngx-layout": "^17.0.1",
        "@types/googlemaps": "^3.43.3",
        "apollo-angular": "^6.0.0",
        "bugsnag-build-reporter": "^2.0.0",
        "dotenv": "^16.4.5",
        "graphql": "^16.8.1",
        "highcharts": "^11.3.0",
        "highcharts-angular": "^4.0.0",
        "lodash-es": "^4.17.21",
        "ngx-progressbar": "^11.0.0",
        "ngx-quill": "^24.0.0",
        "ngx-scrollbar": "^13.0.3",
        "quill": "^1.3.7",
        "rxjs": "^7.8.0",
        "shelljs": "^0.8.4",
        "snazzy-info-window": "^1.1.1",
        "striptags": "^4.0.0-alpha.4",
        "swiper": "^11.0.6",
        "tslib": "^2.6.2",
        "yargs": "^17.6.2",
        "zone.js": "~0.14.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.3.4",
        "@angular-eslint/builder": "17.2.1",
        "@angular-eslint/eslint-plugin": "17.2.1",
        "@angular-eslint/eslint-plugin-template": "17.2.1",
        "@angular-eslint/schematics": "17.2.1",
        "@angular-eslint/template-parser": "17.2.1",
        "@angular/cli": "^17.3.4",
        "@angular/compiler-cli": "^17.3.4",
        "@playwright/test": "^1.41.2",
        "@types/jasmine": "~5.1.4",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.0",
        "@types/quill": "2.0.14",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "eslint": "^8.53.0",
        "jasmine-core": "~5.1.2",
        "karma": "^6.4.3",
        "karma-chrome-launcher": "~3.2.0",
        "karma-cli": "^2.0.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "prettier": "3.2.5",
        "ts-node": "^10.9.2",
        "typescript": "~5.3.3"
    }
}

{
    "name": "dilps-tiresias",
    "private": true,
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "playwright install chromium",
        "dev": "yarn codegen && yarn set-env && yarn clean-env && ng serve",
        "dev-docker": "yarn codegen && yarn set-env && yarn clean-env && ng build --configuration=development --watch --output-hashing=all",
        "prod": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production",
        "prod-map": "yarn codegen && yarn set-env --configuration=production && yarn clean-env && ng build --configuration=production,map --output-hashing=none",
        "staging": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging",
        "staging-map": "yarn codegen && yarn set-env --configuration=staging && yarn clean-env && ng build --configuration=staging,map --output-hashing=none",
        "test": "yarn codegen && ng test",
        "lint": "ng lint --max-warnings 0",
        "e2e": "playwright test",
        "codegen": "./bin/dump-schema && graphql-codegen --config codegen.ts",
        "set-env": "ts-node scripts/setenv.ts",
        "clean-env": "prettier --write client/environments/*",
        "report-build": "bugsnag-build-reporter -n 'Dilps/Tiresias Deployer'"
    },
    "dependencies": {
        "@angular/animations": "^18.2.3",
        "@angular/cdk": "^18.2.3",
        "@angular/common": "^18.2.3",
        "@angular/compiler": "^18.2.3",
        "@angular/core": "^18.2.3",
        "@angular/forms": "^18.2.3",
        "@angular/google-maps": "18.1.0",
        "@angular/localize": "^18.2.3",
        "@angular/material": "^18.2.3",
        "@angular/platform-browser": "^18.2.3",
        "@angular/platform-browser-dynamic": "^18.2.3",
        "@angular/router": "^18.2.3",
        "@apollo/client": "~3.10.8",
        "@bugsnag/core": "^7.25.0",
        "@bugsnag/js": "^7.25.0",
        "@bugsnag/plugin-angular": "^7.25.0",
        "@ecodev/angular-natural-gallery": "^21.0.0",
        "@ecodev/natural": "^61.1.2",
        "@ecodev/natural-layout": "^2.0.2",
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
        "@graphql-codegen/typescript-operations": "^4.2.3",
        "@graphql-tools/mock": "^9.0.3",
        "@types/googlemaps": "^3.43.3",
        "apollo-angular": "^7.0.2",
        "bugsnag-build-reporter": "^2.0.0",
        "dotenv": "^16.4.5",
        "graphql": "^16.9.0",
        "highcharts": "^11.4.6",
        "highcharts-angular": "^4.0.0",
        "lodash-es": "^4.17.21",
        "ngx-progressbar": "^11.0.0",
        "ngx-quill": "^26.0.5",
        "ngx-scrollbar": "^15.1.0",
        "quill": "^2.0.2",
        "rxjs": "^7.8.0",
        "shelljs": "^0.8.4",
        "snazzy-info-window": "^1.1.1",
        "striptags": "^4.0.0-alpha.4",
        "swiper": "^11.1.5",
        "tslib": "^2.6.3",
        "type-fest": "^4.21.0",
        "yargs": "^17.6.2",
        "zone.js": "~0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.3",
        "@angular/cli": "^18.2.3",
        "@angular/compiler-cli": "^18.2.3",
        "@playwright/test": "^1.45.1",
        "@types/jasmine": "~5.1.4",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.14.10",
        "angular-eslint": "18.1.0",
        "eslint": "^9.6.0",
        "jasmine-core": "~5.1.2",
        "karma": "^6.4.3",
        "karma-chrome-launcher": "~3.2.0",
        "karma-cli": "^2.0.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "postcss": "^8.4.39",
        "prettier": "3.3.3",
        "prettier-plugin-css-order": "^2.1.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.5.3",
        "typescript-eslint": "8.0.0-alpha.38"
    }
}

{
    "name": "ecodev/dilps",
    "description": "Dilps",
    "type": "project",
    "homepage": "https://github.com/ecodev/dilps",
    "license": "MIT",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "laminas/laminas-component-installer": true
        }
    },
    "scripts": {
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "check": [
            "php-cs-fixer fix --ansi --dry-run --diff",
            "phpstan analyse --ansi",
            "@test"
        ],
        "fix": [
            "php-cs-fixer fix --ansi"
        ],
        "clear-config-cache": "php bin/clear-config-cache.php",
        "serve": "php -S 0.0.0.0:8080 -t htdocs htdocs/index.php",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "server/Application/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "tests/ApplicationTest/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-PDO": "*",
        "ext-calendar": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.3",
        "creof/doctrine2-spatial": "^1.2",
        "ecodev/felix": "^7.0",
        "ecodev/graphql-upload": "^6.1",
        "imagine/imagine": "dev-develop#60a78ce7973eb6b818f40a2858b85b7b9774a676 as 1.2",
        "laminas/laminas-component-installer": "^2.6",
        "laminas/laminas-config-aggregator": "^1.7",
        "laminas/laminas-diactoros": "^2.8",
        "laminas/laminas-permissions-acl": "^2.9",
        "mezzio/mezzio": "^3.10",
        "mezzio/mezzio-fastroute": "^3.5",
        "mezzio/mezzio-helpers": "^5.8",
        "mezzio/mezzio-laminasviewrenderer": "^2.4",
        "mezzio/mezzio-session": "^1.6",
        "mezzio/mezzio-session-ext": "^1.12",
        "phpoffice/phppresentation": "^1.0",
        "phpoffice/phpspreadsheet": "^1.22",
        "roave/psr-container-doctrine": "^3.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "@stable",
        "laminas/laminas-development-mode": "^3.6",
        "phpstan/phpstan-doctrine": "@stable",
        "phpunit/phpunit": "^9.5"
    }
}
